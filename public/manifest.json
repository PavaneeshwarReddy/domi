{
    "manifest_version": 3,
    "name": "My Vite Extension",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "action" : {
        "default_popup" : "index.html"
    },
    "permissions" : [
        "scripting",
        "downloads",
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions" : [
        "https://*/*",
        "http://*/*"
    ]
  }